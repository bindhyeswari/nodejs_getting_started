<html>
    <head>
        <title></title>
    </head>
    <body>

        <% if (err) { %>
            <div><%= err %></div>
        <% } else { %>
            <% for (var i = 0; i < questions.length; i++) { %>
            <div class="questionContainer">
                <div class="question">
                    <%= questions[i].question %>
                </div>
                <ul class="options">
                    <% for (var j = 0; j < questions[i].options.length; j++ ) { %>
                    <li><input type="radio" name="<%= 'radio_' + i %>" value="<%= j %>"/>
                        <%= questions[i].options[j] %>
                    </li>
                    <% } %>
                </ul>
            </div>
            <% } %>
        <% } %>



    </body>
</html>